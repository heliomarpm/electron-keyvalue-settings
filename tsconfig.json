{
  "compilerOptions": {
    "strict": true,
    "useDefineForClassFields": true,
    "forceConsistentCasingInFileNames": true, 
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "**/*.test.ts"
  ]
}
